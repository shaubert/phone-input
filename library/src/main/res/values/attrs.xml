<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="pi_customCountries" format="boolean|reference"/>

    <declare-styleable name="pi_PhoneInputStyle">
        <attr name="pi_countryIso" format="string|reference"/>
        <attr name="pi_autoChangeCountry" format="boolean|reference"/>
        <attr name="pi_displayCountryCode" format="boolean|reference"/>
        <attr name="pi_customCountries"/>
        <attr name="pi_phoneNumberFormat" format="enum">
            <enum name="e164" value="0"/>
            <enum name="international" value="1"/>
            <enum name="national" value="2"/>
            <enum name="rfc3966" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="pi_CountryPickerStyle">
        <attr name="pi_customCountries"/>
    </declare-styleable>

    <declare-styleable name="pi_PhoneInputLayoutStyle">
        <attr name="pi_customCountries"/>
    </declare-styleable>

</resources>